        </sprite>

        <objects>
            <object id="drwily-sign" w="32" h="32">
                <frame sprite="drwily-sign-blue" duration=".5"/>
                <frame sprite="drwily-sign-cyan" duration=".5"/>
            </object>

            <object id="single-block-1" w="16" h="16">
                <frame sprite="single-block-blue" duration="2"/>
                <frame sprite="single-block-cyan" duration="1"/>
            </object>
            <object id="single-block-2" w="16" h="16">
                <frame sprite="single-block-cyan" duration="1"/>
                <frame sprite="single-block-blue" duration="2"/>
            </object>
            <object id="single-block-3" w="16" h="16">
                <frame sprite="single-block-blue" duration="1"/>
                <frame sprite="single-block-cyan" duration="1"/>
                <frame sprite="single-block-blue" duration="1"/>
            </object>

            <object id="triple-block-horiz-1" w="48" h="16">
                <frame sprite="triple-block-blue-horiz" duration="2"/>
                <frame sprite="triple-block-cyan-horiz" duration="1"/>
            </object>
            <object id="triple-block-horiz-2" w="48" h="16">
                <frame sprite="triple-block-cyan-horiz" duration="1"/>
                <frame sprite="triple-block-blue-horiz" duration="2"/>
            </object>
            <object id="triple-block-horiz-3" w="48" h="16">
                <frame sprite="triple-block-blue-horiz" duration="1"/>
                <frame sprite="triple-block-cyan-horiz" duration="1"/>
                <frame sprite="triple-block-blue-horiz" duration="1"/>
            </object>

            <object id="triple-block-vert-1" w="16" h="48">
                <frame sprite="triple-block-blue-vert" duration="2"/>
                <frame sprite="triple-block-cyan-vert" duration="1"/>
            </object>
            <object id="triple-block-vert-2" w="16" h="48">
                <frame sprite="triple-block-cyan-vert" duration="1"/>
                <frame sprite="triple-block-blue-vert" duration="2"/>
            </object>
            <object id="triple-block-vert-3" w="16" h="48">
                <frame sprite="triple-block-blue-vert" duration="1"/>
                <frame sprite="triple-block-cyan-vert" duration="1"/>
                <frame sprite="triple-block-blue-vert" duration="1"/>
            </object>
        </objects>
    </sprites>

    <layout>
