            <rect x="0" y="0" w="16" h="16" />
        </collision>
        <textures>
            <texture id="telly" url="telly.png" w="128" h="16">
                <animations>
                    <animation id="spin" default="true">
                        <frame x="80" y="0" duration=".16" />
                        <frame x="64" y="0" duration=".16" />
                        <frame x="48" y="0" duration=".16" />
                        <frame x="32" y="0" duration=".16" />
