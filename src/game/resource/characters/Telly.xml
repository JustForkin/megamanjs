                    </animation>
                </animations>
            </texture>
        </textures>
    </model>

    <traits>
        <trait source="Invincibility" id="invincibility" duration="2" />
        <trait source="Jump" id="jump" force="180"/>
        <trait source="Physics" id="physics" />
        <trait source="Weapon" id="weapon" />
    </traits>
</character>
